{
  "name": "pdfpress",
  "version": "0.0.1",
  "description": "the pdfpress js client library",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "test": "nyc mocha lib/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pdfpress/pdfpress-js.git"
  },
  "keywords": [
    "pdf",
    "generation",
    "html",
    "pdfpress"
  ],
  "author": "Matthew Brown <me@matthewbrown.io>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pdfpress/pdfpress-js/issues"
  },
  "homepage": "https://github.com/pdfpress/pdfpress-js#readme",
  "devDependencies": {
    "@types/chai": "^4.1.7",
    "@types/jest": "^23.3.10",
    "@types/mocha": "^5.2.5",
    "@types/nock": "^9.3.0",
    "@types/node": "^10.12.18",
    "chai": "^4.2.0",
    "coveralls": "^3.0.2",
    "mocha": "^5.2.0",
    "nock": "^10.0.5",
    "nyc": "^13.1.0",
    "source-map-support": "^0.5.9",
    "stream-equal": "^1.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.12.0",
    "typescript": "^3.2.2"
  },
  "nyc": {
    "include": [
      "lib/**/*.ts"
    ],
    "exclude": [
      "lib/**/*.test.ts"
    ],
    "extension": [
      ".ts"
    ],
    "require": [
      "ts-node/register"
    ],
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
